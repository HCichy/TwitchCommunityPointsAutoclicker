{
    "name": "Twitch community points autoclicker",
    "version": "0.1",
    "description": "As the above.",
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.twitch.tv/*"
            ],
            "run_at": "document_idle",
            "js": [
                "index.js"
            ]
        }
    ],
    "manifest_version": 2
}